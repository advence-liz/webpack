{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "at first i want build a webpack demo,now i'd like to add something about react",
  "main": "gulpfile.js",
  "env": "dev",
  "scripts": {
    "build": "npm run webpack",
    "restore": "npm install",
    "build:dev": "webpack --progress --colors",
    "build:dist": "webpack --progress --profile --colors",
    "webpack": "gulp webpack",
    "lint": "gulp lint",
    "copy": "gulp copy",
    "test": "npm run lint&&npm run webpack&&launch",
    "start": "webpack-dev-server --inline --hot",
    "module": "webpack --config webpack.module.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/advence-liz/webpack"
  },
  "keywords": [
    "react",
    "webpack",
    "eslint"
  ],
  "pattern": "dev",
  "author": "liz",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.8.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bundle-loader": "^0.5.5",
    "copy-to-clipboard": "^3.0.5",
    "crypto-js": "^3.1.9-1",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "yargs": "^8.0.2"
  }
}
