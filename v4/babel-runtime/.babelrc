{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "> 1%", 
            "last 2 versions", 
            "iOS >= 8",
            "Android >= 4"]
        }
      }
    ],
    "react",
    "stage-1"
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": true,
      "polyfill": true,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}